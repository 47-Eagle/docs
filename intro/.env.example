# App Configuration
NEXT_PUBLIC_APP_NAME="GODDOG Arbitrage"
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NEXT_PUBLIC_APP_ENV="development"

# API Configuration (Replace with your actual endpoints)
NEXT_PUBLIC_API_URL="http://localhost:8000"
NEXT_PUBLIC_WS_URL="ws://localhost:8000"

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_TRADING=false

# External Services (Add your service endpoints)
# Example: NEXT_PUBLIC_SERVICE_NAME="https://api.service.com"

# Rate Limits (requests per minute)
API_RATE_LIMIT=60
WS_RATE_LIMIT=100

# Cache Configuration (in seconds)
CACHE_DURATION=3600

# DO NOT ADD SENSITIVE INFORMATION HERE
# The following are examples of what should NOT be included:
# - API Keys
# - Private Keys
# - Secrets
# - Access Tokens
# - Database Credentials 